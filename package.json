{
  "name": "tslint-customization",
  "version": "1.0.1",
  "description": "TSLint of  customization ",
  "main": "recommendedRuleset.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest"
  },
  "keywords": [
    "tslint",
    "typescript",
    "import",
    "customization"
  ],
  "author": "wfsovereign",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.10",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.6"
  },
  "files": ["dist"],
  "dependencies": {
    "tslint": "^5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/**/*.spec.(ts|tsx)"
    ]
  }
}
